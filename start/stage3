# set -e +a +m +s +i -f
# isFirstRun
# VAR_*
# All functions in /start/functions

if [ "$isFirstRun" == "true" ]
then
   initMysqlConfig
   if [ ! -s "$(removeSingleQuotes "$VAR_param_data_directory")/PG_VERSION" ]
   then
      initPgData
   fi
   tryDelete /initdb
fi
if [ "$(isInsecure)" == "true" ]
then
   printSecurityWarning
fi
